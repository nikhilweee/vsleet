{
  "name": "vsleet",
  "displayName": "vsleet",
  "publisher": "nikhilweee",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikhilweee/vsleet-vscode.git"
  },
  "icon": "media/vsleet.png",
  "description": "Submit LeetCode problems directly from VSCode",
  "version": "0.1.13",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "keywords": [
    "leetcode",
    "python"
  ],
  "preview": true,
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsleet.load",
        "title": "vsleet: New Problem"
      },
      {
        "command": "vsleet.notebook",
        "title": "vsleet: New Notebook"
      },
      {
        "command": "vsleet.login",
        "title": "vsleet: Paste Cookie"
      },
      {
        "command": "vsleet.session",
        "title": "vsleet: Select Session"
      },
      {
        "command": "vsleet.update",
        "title": "vsleet: Update Template"
      },
      {
        "command": "vsleet.local",
        "title": "vsleet: Local Solution",
        "icon": {
          "light": "media/vsleet-light.png",
          "dark": "media/vsleet-dark.png"
        }
      },
      {
        "command": "vsleet.run",
        "title": "vsleet: Run Solution",
        "icon": {
          "light": "media/vsleet-light.png",
          "dark": "media/vsleet-dark.png"
        }
      },
      {
        "command": "vsleet.submit",
        "title": "vsleet: Submit Solution",
        "icon": {
          "light": "media/vsleet-light.png",
          "dark": "media/vsleet-dark.png"
        }
      },
      {
        "command": "vsleet.description",
        "title": "vsleet: Open Description"
      }
    ],
    "configuration": {
      "title": "VSLeet",
      "properties": {}
    },
    "menus": {
      "editor/title/run": [
        {
          "command": "vsleet.local",
          "group": "2_leetcode",
          "when": "editorTextFocus"
        },
        {
          "command": "vsleet.run",
          "group": "2_leetcode",
          "when": "editorTextFocus"
        },
        {
          "command": "vsleet.submit",
          "group": "2_leetcode",
          "when": "editorTextFocus"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vsleet.local",
        "key": "shift+enter",
        "mac": "shift+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "vsleet.run",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "vsleet.submit",
        "key": "ctrl+shift+enter",
        "mac": "cmd+shift+enter",
        "when": "editorTextFocus"
      }
    ],
    "icons": {
      "vsleet-logo": {
        "description": "vsleet Icon",
        "default": {
          "fontPath": "media/vsleet.woff",
          "fontCharacter": "\\e900"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish patch"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "22.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.15.0",
    "glob": "^11.0.0",
    "mocha": "^10.8.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "json5": "^2.2.3"
  }
}
